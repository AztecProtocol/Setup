      "js",
      "jsx",
      "json",
      "node"
    ],
    "rootDir": "./src"
  },
  "dependencies": {
    "@koa/cors": "^3.0.0",
    "@types/node": "^12.6.8",
    "async-mutex": "^0.1.3",
