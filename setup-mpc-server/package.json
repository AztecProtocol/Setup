    "koa": "^2.7.0",
    "koa-body": "^4.1.0",
    "koa-compress": "^3.0.0",
    "koa-router": "^7.4.0",
    "moment": "^2.24.0",
    "path": "^0.12.7",
    "q": "^1.5.1",
    "seedrandom": "^3.0.3",
    "stream-meter": "^1.0.4",
    "tsc-watch": "^2.2.1",
    "typescript": "^3.5.3",
    "web3x": "^4.0.3"
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest": "^24.0.15",
    "@types/koa": "^2.0.49",
    "@types/koa-compress": "^2.0.9",
    "@types/koa-router": "^7.0.42",
    "@types/seedrandom": "^2.4.28",
    "@types/stream-meter": "^0.0.21",
    "@types/supertest": "^2.0.8",
    "jest": "^24.8.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0"
  }
